package abstraction;

// Class name with SbiBank implements the Interface 

public class SbiBank implements Bank  {
	@Override
	public void makePayment() {
		System.out.println("Abstraction is achieved through in");    
		
	}
	public static void main(String[] args) {
		Bank b = new SbiBank();
		b.makePayment();
		
		
		
	}
	

}
