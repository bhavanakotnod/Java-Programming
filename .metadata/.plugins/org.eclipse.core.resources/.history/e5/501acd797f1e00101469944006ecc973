package mock.practice;

public class Prime {
public void CheckPrime(int num) {
	
	int temp=0;
	for(int i=2;i<=num;i++) {
		if((num%i)!=0) {
			temp+=1;
		}
	}
	
	if (temp>0) {
		System.out.println("the given number is prime number");
	}else {
		System.out.println("the given number is consonent");
	}
}


public static void main(String[] args) {

	Prime P=new Prime();
	P.CheckPrime(6);
}
}
