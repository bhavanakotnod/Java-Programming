package abstraction;

public class TransportTicket {
	public static void main(String[] args) {
		Transport t;
		System.out.println("Bus ticket.........");
	   t = new Bus();
		t.bookTicket();
		System.out.println("Train Ticket......");
		t = new Train();
		t.bookTicket();
		System.out.println("Flight ticket..............");
		t = new Flight();
		t.bookTicket();
	}

}
